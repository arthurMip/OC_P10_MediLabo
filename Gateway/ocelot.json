{
    "Routes": [
        {
            "UpstreamPathTemplate": "/api/auth/{all}",
            "DownstreamPathTemplate": "/api/auth/{all}",
            "DownstreamScheme": "http",
            "UpstreamHttpMethod": [
                "POST"
            ],
            "DownstreamHostAndPorts": [
                {
                    "Host": "authapi",
                    "Port": 8080
                }
            ]
        },
        {
            "UpstreamPathTemplate": "/api/patients/{all}",
            "DownstreamPathTemplate": "/api/patients/{all}",
            "DownstreamScheme": "http",
            "UpstreamHttpMethod": [
                "GET",
                "POST",
                "PUT"
            ],
            "DownstreamHostAndPorts": [
                {
                    "Host": "patientapi",
                    "Port": 8080
                }
            ]
        },
        {
            "UpstreamPathTemplate": "/api/notes/{all}",
            "DownstreamPathTemplate": "/api/notes/{all}",
            "DownstreamScheme": "http",
            "UpstreamHttpMethod": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "DownstreamHostAndPorts": [
                {
                    "Host": "noteapi",
                    "Port": 8080
                }
            ]
        },
        {
            "UpstreamPathTemplate": "/api/diabetes-reports",
            "DownstreamPathTemplate": "/api/diabetes-reports",
            "DownstreamScheme": "http",
            "UpstreamHttpMethod": [
                "POST"
            ],
            "DownstreamHostAndPorts": [
                {
                    "Host": "reportapi",
                    "Port": 8080
                }
            ]
        }
    ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7000"
  }
}